local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("Tower FL / kulyusuf#1921", "Ocean")

local Player = Window:NewTab("Player")
local Halos = Window:NewTab("Halos")
local Tags = Window:NewTab("Tags")
local Effects = Window:NewTab("Effects")

local Money = Effects:NewSection("All Effects Free")
local info = Player:NewSection("Speed/Jump")
local haloss = Halos:NewSection("All Halos")
local tagss = Tags:NewSection("All Tags")

info:NewSlider("WalkSpeed", "Your WalkSpeed", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)
info:NewSlider("JumpPower", "Your JumpPower", 500, 0, function(j) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = j
end)

info:NewButton("Finish", "Tp to Finish", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.workspace.tower.sections.finish.FinishGlow.CFrame
end)

haloss:NewButton("Blue Halo", "", function()
    local args = {
    [1] = "BlueHalo"
}

game:GetService("ReplicatedStorage").haloAction:FireServer(unpack(args))
end)
